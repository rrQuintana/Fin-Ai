{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*" : ["./assets/*"],
      "@interfaces/*" : ["./interfaces/*"],
      "@components/*" : ["./components/*"],
      "@stacks/*" : ["./stacks/*"],
      "@pages/*" : ["./pages/*"],
      "@hooks/*" : ["./src/hooks/*"],
      "@src/*" : ["./src/*"],
      "@tabs/*" : ["./tabs/*"],
    }
  }
}
